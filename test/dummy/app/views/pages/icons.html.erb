<section>
  <h1 class="typography-h1">Icons</h1>
  <div class="grid grid-cols-2 gap-4">
    <% @icons.each do |icon| %>
      <div class="flex items-center gap-2 p-2 rounded">
        <%= render RapidUI::Icon.new(icon, size: 24) %>
        <code class="typography-code"><%= icon %></code>
      </div>
    <% end %>
  </div>
</section>
